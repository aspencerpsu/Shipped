<!DOCTYPE html>
<html>
<head>
  <title>ShippingValidations</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if flash[:notice] %>   
    <div class="flash_notice" style="color: red">       
    	<%=flash[:notice] %>     
    </div>  
     <% end %>  
     <% if flash[:alert] %>    
      <div class="flash_alert" style="color: red">      
       	<%=flash[:alert] %>     
       </div>   
       <% end %>
<%= yield %>

</body>
</html>
